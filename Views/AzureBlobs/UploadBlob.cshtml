<h1 style="color:blue">
    Upload Blob
</h1>

<a asp-controller="AzureBlobs"
   asp-action="ListBlobs"
   asp-route-containerName="@ViewData["CONTAINERNAME"]">
    Volver a Blobs
</a>

<form method="post" enctype="multipart/form-data">
    <input type="file" name="file" class="form-control" />
    <input type="hidden" name="containerName" value="@ViewData["CONTAINERNAME"]" />
    <button type="submit" class="btn btn-info">
        Upload Blob
    </button>
</form>
